{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "capstoneproject-factory"
		},
		"RDS_db_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RDS_db'"
		},
		"my_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'my_blob'"
		},
		"wcd_public_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'wcd_public_blob'"
		},
		"RDS_db_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "de-rds.czm23kqmbd6o.ca-central-1.rds.amazonaws.com"
		},
		"RDS_db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "stack"
		},
		"RDS_db_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "postgres"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "capstoneproject-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"RDS_db_password": {
						"value": "[parameters('RDS_db_password')]"
					},
					"my_blob_connectionString": {
						"value": "[parameters('my_blob_connectionString')]"
					},
					"wcd_public_blob_connectionString": {
						"value": "[parameters('wcd_public_blob_connectionString')]"
					},
					"RDS_db_properties_typeProperties_server": {
						"value": "[parameters('RDS_db_properties_typeProperties_server')]"
					},
					"RDS_db_properties_typeProperties_database": {
						"value": "[parameters('RDS_db_properties_typeProperties_database')]"
					},
					"RDS_db_properties_typeProperties_username": {
						"value": "[parameters('RDS_db_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}