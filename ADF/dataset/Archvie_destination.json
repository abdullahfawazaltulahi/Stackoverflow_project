		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().fileName",
					"type": "Expression"
				},
				"folderPath": "Archive",
				"container": "stackoverflow-container"
			}
		}
	}
}