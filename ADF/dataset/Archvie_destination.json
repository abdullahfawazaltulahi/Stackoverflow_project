{
	"name": "Archvie_destination",
	"properties": {
		"description": "this dataset will be the destination of files that from source to archive and save it ",
		"linkedServiceName": {
			"referenceName": "my_blob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"fileName": {
				"type": "string"
			}
		},
		"annotations": [],
