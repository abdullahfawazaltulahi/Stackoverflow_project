{
	"name": "Validation_schena",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "post_source",
						"type": "DatasetReference"
					},
					"name": "datasource"
				}
			],
			"sinks": [
				{
					"name": "sinkdataset"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          id as integer,",
				"          AcceptedAnswerId as integer,",
				"          AnswerCount as integer,",
				"          Body as string,",
				"          CommentCount as integer,",
				"          CreationDate as date,",
				"          FavoriteCount as integer,",
				"          LastEditDate as date,",
				"          LastEditorDisplayName as string,",
				"          LastEditorUserId as integer,",
				"          OwnerUserId as integer,",
				"          ParentId as integer,",
				"          PostTypeId as integer,",
				"          Score as float,",
				"          Tags as string,",
				"          Title as string,",
				"          ViewCount as integer",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> datasource",
				"datasource sink(validateSchema: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     store: 'cache',",
				"     format: 'inline',",
				"     output: false,",
				"     saveOrder: 1) ~> sinkdataset"
			]
		}
	}
}