{
	"name": "AF",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "arc_file",
						"type": "DatasetReference"
					},
					"name": "archivingFile",
					"description": "here we will archive the files that in the main folder into archive folder and we keep it for future need."
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "arc_file",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     moveFiles: ['Landing','Archive'],",
				"     rowUrlColumn: 'FileName',",
				"     wildcardPaths:[(concat('stackoverflow-container/Landing/{StackOverFlow-Post,StackOverFlow-PostType,StackOverFlow-User}','/*.csv'))]) ~> archivingFile",
				"archivingFile sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}